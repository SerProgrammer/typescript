{
  "version": "1.0.0",
  "compilerOptions": {
    "outDir": "build/compiled",
    "lib": ["ES6"],
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "declaration": true,
  },
  "exclude": ["build", "node_modules"]
}